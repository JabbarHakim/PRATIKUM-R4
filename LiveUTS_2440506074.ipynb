      "source": [
        "numbers = input(\"Masukkan Nilai = \")\n",
        "\n",
        "genap = 0\n",
        "ganjil = 0\n",
        "\n",
        "for nilai in numbers:\n",
        "  if int(nilai) % 2 == 0:\n",
        "    genap += 1\n",
        "  else:\n",
        "    ganjil += 1\n",
        "\n",
        "print(\"Total Genap adalah = \", genap)\n",
        "print(\"Total Ganjil adalah = \", ganjil)"
